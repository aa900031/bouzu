{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**"
			]
		},
		"dev": {
			"cache": false
		},
		"lint": {
			"outputs": []
		},
		"preview": {
			"outputs": []
		},
		"clean:cache": {
			"cache": false
		}
	}
}
