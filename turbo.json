{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**"
			]
		},
		"dev": {
			"cache": false
		},
		"lint": {
			"outputs": []
		},
		"preview": {
			"outputs": []
		},
		"release": {
			"dependsOn": ["^release"],
			"cache": false
		}
	}
}
